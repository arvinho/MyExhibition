<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/signup_titlebar_background">
    <ImageView
            android:id="@+id/signup_page_titltbar_iv_telephone"
            android:background="@drawable/new_telephone_unfocus"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    <ImageView
            android:id="@+id/signup_page_titltbar_iv_information"
            android:background="@drawable/information_focus"
            android:layout_toLeftOf="@id/signup_page_titltbar_iv_telephone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
</RelativeLayout>